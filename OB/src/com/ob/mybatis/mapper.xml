<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- mapper 태그의 namespace 속성 필수 작성 -->
<mapper namespace="Room">
<!-- 예시  -->
	<select id="list" resultType="com.ob.vo.RoomVO" >
		SELECT * FROM ARS_T ORDER BY A_IDX
	</select>
	<select id="notice" resultType="com.ob.vo.NoticeVO">
		SELECT * FROM OBHOTEL_BOARD WHERE TYPE = #{type}
	</select>
</mapper>  






